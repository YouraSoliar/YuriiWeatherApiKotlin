<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="presentation.FragmentConstance">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textViewConst1"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:background="@color/teal_700"
            android:gravity="center"
            android:text="@string/text_view_hold"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/textViewConst2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewConst2"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:background="@color/teal_700"
            android:gravity="center"
            android:text="@string/text_view_hold"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewConst1" />

        <TextView
            android:id="@+id/textViewConst3"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/teal_700"
            android:gravity="center"
            android:text="@string/text_view_hold"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/textViewConst4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewConst1" />

        <TextView
            android:id="@+id/textViewConst4"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@color/teal_700"
            android:gravity="center"
            android:text="@string/text_view_hold"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewConst2"
            app:layout_constraintStart_toEndOf="@+id/textViewConst3"/>
    </androidx.constraintlayout.widget.ConstraintLayout>



</FrameLayout>